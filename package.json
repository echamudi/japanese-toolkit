    "kanji-tree": "./bin/kanji-tree.js"
  },
  "files": [
    "/bin",
    "/lib",
    "/index.js",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/echamudi/kanji#readme",
  "devDependencies": {
    "typescript": "^3.9.5",
    "@types/node": "^14.0.14",
